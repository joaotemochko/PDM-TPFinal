<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TpFinal.ResultadoView">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Qual ID procurar" />
            <Button Text="Pesquisar" Clicked="BtnPesquisar_Clicked"/>
            <Label Text="{Binding PersonID}" />
            <Label Text="{Binding NomeProduto}" />
            <Label Text="{Binding Peso}" />
            <Label Text="{Binding NomeProdutor}" />
            <Label Text="{Binding Email}" />
            <Label Text="{Binding NCM}" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>